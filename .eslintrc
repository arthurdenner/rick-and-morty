{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "react/no-children-prop": 0,
    "react/forbid-prop-types": 0,
    "no-return-assign": 0,
    "jsx-a11y/accessible-emoji": 0
  }
}
